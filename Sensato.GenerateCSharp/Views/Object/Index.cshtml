@model IEnumerable<Sensato.GenerateCSharp.Models.Tb_Objects>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.ObjectName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Tb_Contexts.ContextName)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.ObjectName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Tb_Contexts.ContextName)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.ID_Object }) |
            @Html.ActionLink("Details", "Details", new { id=item.ID_Object }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.ID_Object })
        </td>
    </tr>
}

</table>
